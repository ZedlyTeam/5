							Статус сервера						</span><!-- left-sidebar-title -->
					<div id="serverTime" class="border-bottom" timeserver="2024-08-26 21:18:59">21:18:59</div>
<script>
    document.addEventListener('DOMContentLoaded', function() {
        var $timeServerSecond = 'none',
            element = document.getElementById('serverTime');
        switchServerTime();
        setInterval(switchServerTime, 1000);
        function switchServerTime() {
            let date = new Date();
            if ($timeServerSecond === 'none') {
                let offset = new Date(element.getAttribute('timeserver'));
                $timeServerSecond = (date.getTime() - offset.getTime());
            }
            let myTime = 0;
            if ($timeServerSecond < 0) myTime = (date.getTime() + ($timeServerSecond * -1))
            else myTime = (date.getTime() - $timeServerSecond)

            $newDate = new Date(myTime);

            let hours = $newDate.getHours(),
                minutes = $newDate.getMinutes(),
                seconds = $newDate.getSeconds();

            if (hours < 10) hours = '0' + hours;
            if (minutes < 10) minutes = '0' + minutes;
            if (seconds < 10) seconds = '0' + seconds;

            element.innerHTML = hours + ':' + minutes + ':' + seconds;
        }
    });
</script>	
					
